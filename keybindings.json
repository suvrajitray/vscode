  {
    "command": "editor.action.formatDocument",
    "key": "space c f",
    "when": "editorTextFocus && neovim.init && neovim.mode != 'insert'"
  },
  {
    "command": "editor.debug.action.toggleBreakpoint",
    "key": "space d b",
    "when": "editorTextFocus && neovim.init && neovim.mode != 'insert'"
  },
  {
    "args": "<C-d>",
    "command": "vscode-neovim.send",
    "key": "ctrl+d",
    "when": "neovim.init && editorFocus"
  },
  {
    "args": "<CS-d>",
    "command": "vscode-neovim.send",
    "key": "ctrl+shift+d",
    "when": "neovim.init && editorFocus"
  },
  {
    "args": "<CS-l>",
    "command": "vscode-neovim.send",
    "key": "ctrl+shift+l",
    "when": "neovim.init && editorFocus"
  },
  {
    "key": "ctrl+j",
